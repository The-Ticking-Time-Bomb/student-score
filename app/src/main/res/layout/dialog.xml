<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
    
    <RelativeLayout
            android:layout_marginTop="7dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
        
        <TextView
                android:id="@+id/xingming"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="9dp"
                android:text="姓名:"
                android:textSize="18dp"/>
        
        <EditText
                android:id="@+id/name123"
                android:textSize="18dp"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@id/xingming"/>
        
        <TextView
                android:id="@+id/xuehao2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="学号:"
                android:textSize="18dp"
                android:layout_below="@id/xingming"
                android:layout_marginTop="20dp"/>
        
        <EditText
                android:id="@+id/xuehao123"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@id/xuehao2"
                android:layout_below="@id/name123"
                android:textSize="18dp"/>
        
        <TextView
                android:id="@+id/age2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="年龄:"
                android:textSize="18dp"
                android:layout_toRightOf="@id/name123"
                android:layout_marginTop="9dp"
                android:layout_marginRight="10dp"/>
        
        <EditText
                android:id="@+id/age123"
                android:textSize="18dp"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@id/age2"/>
        
        <TextView
                android:id="@+id/phone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="电话:"
                android:layout_below="@id/age2"
                android:textSize="18dp"
                android:layout_toRightOf="@id/xuehao123"
                android:layout_marginTop="21dp"/>
        
        <EditText
                android:id="@+id/dianhua123"
                android:textSize="18dp"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@id/phone"
                android:layout_below="@id/age123"/>
        
        <TextView
                android:id="@+id/className"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="18dp"
                android:text="班级:"
                android:layout_below="@id/xuehao2"
                android:layout_alignLeft="@id/xuehao2"
                android:layout_marginTop="21dp"/>
        
        <EditText
                android:id="@+id/class_Name123"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@id/className"
                android:layout_below="@id/xuehao123"
                android:textSize="18dp"/>
        
        <TextView
                android:id="@+id/password"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="密码:"
                android:textSize="18dp"
                android:layout_marginTop="21dp"
                android:layout_below="@id/xuehao123"
                android:layout_toRightOf="@+id/class_Name123"
                android:layout_marginLeft="10dp"
               />
        
        <EditText
                android:id="@+id/key"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:textSize="18dp"
                android:layout_below="@id/dianhua123"
                android:layout_toRightOf="@id/password"/>
        
        <TextView
                android:id="@+id/address"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="地址:"
                android:textSize="18dp"
                android:layout_below="@id/className"
                android:layout_marginTop="20dp"/>
        
        <EditText
                android:id="@+id/dizhi123"
                android:textSize="18dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@id/address"
                android:layout_below="@id/class_Name123"/>
        
        <TextView
                android:id="@+id/shuxue1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/shuxu"
                android:textSize="18dp"
                android:layout_below="@id/address"
                android:layout_alignLeft="@id/address"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="20dp"
                />
        
        <EditText
                android:id="@+id/shuxue123"
                android:layout_width="80dp"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@id/shuxue1"
                android:textSize="18dp"
                android:layout_below="@id/address"/>
        
        <TextView
                android:id="@+id/computer1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@id/shuxue123"
                android:layout_marginLeft="2dp"
                android:layout_marginTop="20dp"
                android:layout_below="@id/address"
                android:textSize="18dp"
                android:text="安卓:"/>
        
        <EditText
                android:id="@+id/computer123"
                android:layout_width="80dp"
                android:layout_height="wrap_content"
                android:textSize="18dp"
                android:layout_toRightOf="@id/computer1"
                android:layout_below="@id/address"/>
        
        <TextView
                android:id="@+id/English1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="18dp"
                android:layout_marginTop="20dp"
                android:text="英语:"
                android:layout_below="@id/address"
                android:layout_toRightOf="@id/computer123"/>
        
        <EditText
                android:id="@+id/English123"
                android:layout_width="80dp"
                android:layout_height="wrap_content"
                android:layout_below="@id/address"
                android:layout_toRightOf="@id/English1"
                android:textSize="18dp"/>
    
    
    </RelativeLayout>
</RelativeLayout>